{
  "name": "Guild Preview Extension",
  "version": "0.1.0",
  "description": "Preview a guild tab on a website",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Guildify"
  }
}