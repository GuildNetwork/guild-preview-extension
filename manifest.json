{
  "name": "Slick Preview Extension",
  "version": "0.4.4",
  "description": "Preview Slick widgets on a website",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "webNavigation",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}